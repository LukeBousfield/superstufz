  "bugs": {
    "url": "https://github.com/helenphan0/tomatotime/issues"
  },
  "homepage": "https://github.com/helenphan0/tomatotime#readme",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
